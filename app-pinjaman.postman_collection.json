					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse JSON safely and set collection variable when present",
									"let isJson = false;",
									"let body = pm.response.text();",
									"let json = null;",
									"",
									"try {",
									"  json = pm.response.json();",
									"  isJson = true;",
									"} catch (e) {",
									"  isJson = false;",
									"}",
									"",
									"// Tests",
									"pm.test(\"Status code is 200\", function () {",
									"  pm.expect(pm.response.code).to.eql(200);",
									"});",
									"",
									"pm.test(\"Response is JSON\", function () {",
									"  pm.expect(isJson, \"response should be valid JSON\").to.be.true;",
									"});",
									"",
									"pm.test(\"Token exists and is a non-empty string\", function () {",
									"  pm.expect(isJson, \"response must be JSON to check token\").to.be.true;",
									"  pm.expect(json).to.be.an(\"object\");",
									"  pm.expect(json).to.have.property(\"data\").that.is.an(\"object\");",
									"  pm.expect(json.data).to.have.property(\"token\");",
									"  pm.expect(json.data.token).to.be.a(\"string\").and.to.have.length.greaterThan(0);",
									"});",
									"",
									"// Save token to collection variable only for 2xx responses and when token is valid string",
									"if (pm.response.code >= 200 && pm.response.code < 300 && isJson && json && json.data && typeof json.data.token === 'string' && json.data.token.length > 0) {",
									"  pm.collectionVariables.set('authToken', json.data.token);",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
